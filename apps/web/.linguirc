{
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src"
      ]
    }
  ],
  "format": "po",
  "formatOptions": {
    "origins": true,
    "lineNumbers": false
  },
  "orderBy": "origin",
  "locales": [
    "en",
    "es",
    "ta"
  ],
  "sourceLocale": "en",
  "fallbackLocales": {
    "default": "en"
  }
}
