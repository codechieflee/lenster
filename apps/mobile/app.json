{
  "expo": {
    "name": "Lenster",
    "slug": "lenster",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "scheme": "lenster",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
